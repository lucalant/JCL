<body class="page page-id-760 page-template-default logged-in admin-bar custom-background customize-support"
      xmlns="http://www.w3.org/1999/html">

<div id="azure-wrapper">
  <section id="pre-content">
    <div class="pre-content-inner">
      <div class="container">
        <div class="sixteen columns">
          <div class="space-title">
            <h1 span class="page-title"> Distribution points for <%=link_to @home_product.name ,home_products_path(:id => @home_product.id)%></h1>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- main-container -->
  <section id="main-container" class="container" role="main">
    <div id="main" class="sixteen columns content">

      <!-- post entry -->
      <article id="post-760" class="post-760 page type-page status-publish hentry page-single clearfix">
        <div class="transition-content">
          <div class="asc-columns ">
            <div class="asc-frame">
              <% @distributions.each do |id| %>
              <p><strong><a class="transition-link" ><span><%= link_to id.name, distribution_path(:id => id.id ) %>&nbsp;</span></a></strong></p>
              <%end%>
            </div>
          </div>
        </div>
       <section>
        <%= link_to 'Back to rooms',room_home_products_path, {:class => "asc-button small gray"}%> <span>
                            </br></br>

         Back to product <%=link_to @home_product.name ,home_products_path(:id => @home_product.id), {:class => "asc-button small gray"}%></span>
      </section>
      </article>
      <!-- end post entry -->
    </div>
  </section>
</div>
<!-- end main-container -->
</body>