<h1>HomeProduct#rooms</h1>
<p>Find me in app/views/home_product/rooms.html.erb</p>
<section id="transitionlinks">
  <ul>
     <li> link_to All rooms ,   </li>
     <li> previous room </li>
     <li> next room </li>
  </ul>
</section>

<section id="homeProducts">
    <% @home_product_by_room.each do |homeproduct|  %>

          <ul class = "image">

                <%link_to homeproduct_path(homeproduct) do%>
                    <%= image_tag("/homeproduct/#{homeproduct.image_url}") %>
                <%end%>


          </ul>
          <ul class="title"> <%= link_to homeproduct.name, homeproduct_path(homeproduct) %> </ul>


    <%end%>
</section>
